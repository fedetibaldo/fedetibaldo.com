{
  "name": "gatsby-starter-ghost",
  "description": "A starter template to build lightning fast websites with Ghost and Gatsby",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Ghost Foundation",
  "homepage": "https://docs.ghost.org/api/gatsby/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tryghost/gatsby-starter-ghost.git"
  },
  "resolutions": {
    "sharp": "0.23.4"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "bugs": {
    "url": "https://github.com/tryghost/gatsby-starter-ghost/issues"
  },
  "keywords": [
    "gatsby",
    "ghost"
  ],
  "main": "n/a",
  "scripts": {
    "deploy": "npm run deploy:gatsby && npm run deploy:projects",
    "deploy:projects": "mkdir public/connection && mkdir public/codepen-assets && npm run projects:bootstrap && npm run projects:build && npm run projects:publish",
    "deploy:gatsby": "npm run gatsby:build",
    "projects:bootstrap": "lerna bootstrap",
    "projects:build": "lerna run build",
    "projects:publish": "cp -r packages/connection/dist/* public/connection && cp -r packages/codepen-assets/assets/* public/codepen-assets",
    "gatsby:build": "gatsby build",
    "gatsby:dev": "GATSBY_CMS_ENV=development gatsby develop",
    "gatsby:serve": "gatsby build && NODE_ENV=production gatsby serve",
    "lint": "eslint . --ext .js --cache",
    "lint:fix": "eslint . --ext .js --cache --fix",
    "cms": "npx netlify-cms-proxy-server",
    "start": "npm run gatsby:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-ghost": "1.0.0",
    "eslint-plugin-react": "7.18.3",
    "lerna": "^3.22.1"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^2.2.6",
    "@formatjs/intl-relativetimeformat": "^5.2.6",
    "@reach/router": "^1.3.3",
    "@tryghost/admin-api": "^1.2.0",
    "@tryghost/helpers": "1.1.22",
    "@tryghost/helpers-gatsby": "1.0.25",
    "cheerio": "1.0.0-rc.3",
    "gatsby": "^2.23.3",
    "gatsby-awesome-pagination": "0.3.5",
    "gatsby-image": "2.2.41",
    "gatsby-plugin-advanced-sitemap": "1.5.2",
    "gatsby-plugin-catch-links": "2.1.26",
    "gatsby-plugin-create-client-paths": "^2.2.3",
    "gatsby-plugin-feed": "2.3.27",
    "gatsby-plugin-force-trailing-slashes": "1.0.4",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-netlify-cms": "^4.5.0",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-postcss": "^2.3.2",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-remote-images": "^2.1.0",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-rehype-prismjs": "^1.1.1",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-source-ghost": "4.0.4",
    "gatsby-transformer-rehype": "^1.4.4",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "2.3.16",
    "lodash": "4.17.19",
    "netlify-cms-app": "^2.14.12",
    "prismjs": "^1.20.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-intl": "^4.6.9",
    "tailwindcss": "^1.4.6",
    "typeface-source-sans-pro": "^1.1.5"
  }
}
